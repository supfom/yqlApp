				padding-left: 15px;
			}
			.empty .logo {
				float: left;
				margin-left: 20px;
			}
			.empty .logo img {
				height: 100px;
			}
			.empty .msg {
				height: 100px;
				line-height: 100px;
				margin-left: 120px;
			}
			.shop {
				background-color: #FFF;
				margin-top: 6px;
			}
			.shop span {
				font-size: 12px;
				text-overflow: ellipsis;
				overflow: hidden;
				white-space: nowrap;
			}
			.shop_name {
				height: 53px;
			}
			.shop_name_left {
			}
			.shop .checkBox {
				display: inline-block;
				background: url(../res/images/checkOn.png) center center no-repeat;
				background-size: 100% 100%;
				width: 17px;
				height: 17px;
				margin: 18px 12px;
			}
			.shop .checkBox.active {
				background: url(../res/images/checkOff.png) center center no-repeat;
				background-size: 100% 100%;
			}
			.shop_name_text {
				font-size: 14px;
				line-height: 53px;
			}
			.shop_item_group {
			}
			.shop_item {
				width: 100%;
				display: -webkit-box;
				-webkit-box-orient: horizontal;
				display: -webkit-flex;
				display: flex;
				position: relative;
				padding: 6px 0;
			}
			.shop_item_left {
				position: relative;
			}
			.item_icon {
				width: 53px;
				margin-left: 4px;
			}
			.shop_item_center {
				position: relative;
				-webkit-box-flex: 1;
				-webkit-flex: 1;
				flex: 1;
				padding-left: 7px;
				width: 0%;
			}
			.shop_item_center .title {
				display: block;
				line-height: 17px;
				text-indent: 0 !important;
			}
			.shop_item_center .description {
				display: block;
				line-height: 17px;
			}
			.shop_item_center .classify {
				display: block;
				color: #9E9E9E;
				line-height: 14px;
			}
			.tag_group .tag {
				height: 12px;
			}
			.shop_item_right {
				text-align: right;
				position: relative;
				width: 50px;
				color: #E46528;
				padding-right: 7px;
			}
			.shop_item_right .price {
				margin-bottom: 10px;
			}
			.pull-left {
				float: left;
			}
			.pull-right {
				float: right;
			}
			.orderNum {
				border-bottom: 1px solid #CCC;
				padding: 4px;
			}
			.money {
				text-align: center;
				padding: 4px;
				line-height: 30px;
				line-height: 20px;
				border-bottom: 1px solid #CCC;
				border-top: 1px solid #CCC;
			}
			.money label {
				color: #6ED6D7;
				padding: 0px 6px;
				font-weight: bold;
			}
			.but_group {
				padding: 6px;
			}
			.but_group a {
				border: #9d9d9d solid 1px;
				color:#9d9d9d;
				font-size: 12px;
				border-radius: 6px;
				padding: 4px 12px;
				margin-left: 6px;
			}
			.but_group .active{
				color: #E46523;
				border: #E46523 solid 1px;
			}
